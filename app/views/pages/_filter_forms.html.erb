<div class="container number filter-div">
	<div class="row">
		<div class="col-sm-8">
			<h3>Filter by <span class="number-id">Age</span></h3>
		</div>
		<button class="btn btn-dark filter-button" id="participant-id-filter">
			Filter by Participant ID
		</button>
	</div>
	<hr>

	<form>
		<div class="form-row">
			<div class="form-group col-md-3">
				<label for="num-from">Select from</label>
				<input type="number" class="form-control" id="num-from" placeholder="0">
			</div>

			<div class="form-group col-md-3">
				<label for="num-to">to</label>
				<input type="number" class="form-control" id="num-to" placeholder="100">
			</div>
		</div>
		<div class="form-row">
			<div class="form-group col-md-3">
				<div class="form-check">
					<label class="form-check-label checkbox-inline" for="null-values">
						<input class="form-check-input" type="checkbox" value="" id="null-values">
						Include null values
					</label>
				</div>
			</div>
			<div class="form-group col-md-3">
				<button type="button" class="btn btn-success go-filter-button">Go</button>
			</div>
		</div>
	</form>
</div>

<div class="container date filter-div">
	<div class="row">
		<div class="col-sm-8">
			<h3>Filter by <span class="date-id"></span></h3>
		</div>
		<button class="btn btn-dark filter-button" id="participant-id-filter">
			Filter by Participant ID
		</button>
	</div>

	<hr>

	<form>
		<div class="form-row">
			<div class="form-group col-md-4">
				<label for="date-from">Start Date</label>
				<input type="date" class="form-control" id="date-from" placeholder="0">
			</div>
			<div class="form-group col-md-4">
				<label for="date-to">End Date</label>
				<input type="date" class="form-control" id="date-to" placeholder="100">
			</div>
		</div>

		<button type="button" class="btn btn-success go-filter-button">Go</button>
	</form>
</div>
